/*
* HOMEPAGE
*/
.colibri-wrapper {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  z-index: -1;
  width: 100%;
  overflow: hidden; }

.colibri-img {
  position: absolute;
  right: -14vw;
  width: 40%;
  width: 75vw;
  min-width: 160px;
  opacity: 0;
  max-height: 1200px;
  animation-name: anim-fullcolibri;
  animation-duration: 800ms;
  animation-delay: 500ms;
  animation-fill-mode: forwards;
  animation-timing-function: cubic-bezier(0.39, 0.58, 0.57, 1); }
  @supports not (display: grid) {
    .colibri-img {
      width: 67vw; } }
  @media (min-width: 768px) {
    .colibri-img {
      top: -14vw; } }
@keyframes anim-fullcolibri {
  from {
    opacity: 0; }
  to {
    opacity: 1; } }

@keyframes anim-colibri {
  from {
    transform: translate(300px, -120px); }
  to {
    transform: translate(0); } }

#animated-colibri {
  animation-name: anim-colibri;
  animation-duration: 800ms;
  animation-delay: 500ms;
  animation-fill-mode: forwards;
  animation-timing-function: cubic-bezier(0.39, 0.58, 0.57, 1); }

.homepage-header {
